<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryDark"
    tools:context=".presencesalarie">

    <ImageView
        android:id="@id/retourimg"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="25dp"
        android:layout_marginStart="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_baseline_arrow_back_24" />
    <ImageView
        android:id="@id/Home"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:src="@drawable/ic_baseline_home_24" />


    <ImageView
        android:id="@id/logodyalna"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@id/Home"
        android:src="@drawable/logodyalna" />





    <ImageView
        android:id="@id/Power"
        android:layout_width="wrap_content"
        android:layout_height="30dp"
        android:layout_marginEnd="10dp"
        android:layout_marginTop="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"

        android:background="@drawable/circle_mov"
        android:src="@drawable/ic_baseline_power_settings_new_24"/>




    <ImageView
        android:id="@id/ImgSeance"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="40dp"
        app:layout_constraintTop_toBottomOf="@id/logodyalna"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_notes_24" />

    <TextView
        android:id="@id/NameSeance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="27dp"
        android:layout_marginStart="40dp"
        android:text="name seance"
        android:textColor="#DAE1E7"
        android:textSize="22dp"
        app:layout_constraintStart_toEndOf="@id/ImgSeance"
        app:layout_constraintTop_toBottomOf="@id/logodyalna" />

    <ImageView
        android:id="@id/ImgHD"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toBottomOf="@id/ImgSeance"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_baseline_watch_later_24" />

    <ImageView
        android:id="@id/ImgFleche"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="20dp"
        android:layout_marginStart="10dp"
        app:layout_constraintTop_toBottomOf="@id/ImgSeance"
        app:layout_constraintStart_toEndOf="@id/HD"
        app:srcCompat="@drawable/ic_flesh" />

    <TextView
        android:id="@id/HD"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:layout_marginStart="10dp"
        android:text="debut"
        android:textColor="#DAE1E7"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@id/ImgHD"
        app:layout_constraintTop_toBottomOf="@id/NameSeance" />

    <ImageView
        android:id="@id/ImgHF"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginStart="15dp"
        app:layout_constraintTop_toBottomOf="@id/ImgSeance"
        app:layout_constraintStart_toEndOf="@id/ImgFleche"
        app:srcCompat="@drawable/ic_baseline_watch_later_24" />

    <TextView
        android:id="@id/HF"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:layout_marginStart="10dp"
        android:text="fin"
        android:textColor="#DAE1E7"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@id/ImgHF"
        app:layout_constraintTop_toBottomOf="@id/NameSeance" />

    <ImageView
        android:id="@id/ImgDate"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="65dp"
        app:layout_constraintTop_toBottomOf="@id/NameSeance"
        app:layout_constraintStart_toStartOf="@id/HF"
        app:srcCompat="@drawable/ic_baseline_date_range_24" />

    <TextView
        android:id="@id/date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android:layout_marginStart="10dp"
        android:text="date"
        android:textColor="#DAE1E7"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@id/ImgDate"
        app:layout_constraintTop_toBottomOf="@id/NameSeance" />

    <TextView
        android:id="@id/Coach"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginStart="50dp"
        android:text="@string/Coach"
        android:textColor="#4DE4C1"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="@id/ImgSeance"
        app:layout_constraintTop_toBottomOf="@id/ImgHF" />

    <TextView
        android:id="@id/NameSalarie"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginStart="30dp"
        android:text="nom du coach"
        android:textColor="#DAE1E7"
        android:textSize="20dp"
        app:layout_constraintStart_toEndOf="@id/Coach"
        app:layout_constraintTop_toBottomOf="@id/ImgHF" />


    <ListView
        android:id="@id/listclient"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Coach"
        app:layout_constraintBottom_toTopOf="@id/BtnSave"/>

    <Button
        android:id="@id/BtnSave"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingLeft="50dp"
        android:paddingRight="50dp"
        android:layout_marginBottom="10dp"
        android:textSize="15dp"
        android:background="@drawable/circle_mov"
        android:layout_marginTop="50dp"
        android:text="@string/Valider"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        />
</androidx.constraintlayout.widget.ConstraintLayout>